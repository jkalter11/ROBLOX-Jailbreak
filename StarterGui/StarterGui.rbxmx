				<Item class="LocalScript" referent="RBX6861B41A05144BF39C637895129597A8">
					<Properties>
						<bool name="Disabled">false</bool>
						<Content name="LinkedSource"><null></null></Content>
						<string name="Name">LocalScript</string>
						<ProtectedString name="Source"><![CDATA[--[[If the button  is clicked, it will make you Warden IF no one else already is.
	To be added: LR menu for the last remaining prisoner.
--]]

script.Parent.MouseButton1Click:connect(function()
	if game.ReplicatedStorage.wardenClaimed.Value == 0 then
		for k,v in pairs(game.Players:GetChildren()) do
			if v.TeamColor.Name == "Lapis" then
				script.Parent.Text = "Someone is already warden"
				wait(5)
				script.Parent.Text = "Claim Warden"
				break
			else
				if game.Players.LocalPlayer.TeamColor.Name == "Really blue" then
					game.Players.LocalPlayer.TeamColor = BrickColor.new("Lapis")
					game.ReplicatedStorage.wardenClaimed.Value = 1
					script.Parent.Text = "You are now warden!"
					wait(5)
					script.Parent.Text = "Claim Warden"
				elseif game.Players.LocalPlayer.TeamColor.Name == "Really red" then
					script.Parent.Text = "Prisoners can't claim warden!"
					wait(5)
					script.Parent.Text = "Claim Warden"
					
				end
			end
		end
	else
	script.Parent.Text = "Warden has already been claimed"
	wait(5)
	script.Parent.Text = "Claim Warden"
	end
end)]]></ProtectedString>
					</Properties>
				</Item>
			</Item>
			<Item class="LocalScript" referent="RBX65F4A6B9EC294652A57A707340323351">
