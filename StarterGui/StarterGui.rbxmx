					<string name="Name">LocalScript</string>
					<ProtectedString name="Source"><![CDATA[function onKeyPress(inputObject)
	if inputObject.KeyCode == Enum.KeyCode.F4 then
		if script.Parent.Visible then
			script.Parent.Visible = false
		elseif not script.Parent.Visible then
			script.Parent.Visible = true
		end
	end
end

game:GetService("UserInputService").InputBegan:connect(onKeyPress)]]></ProtectedString>
				</Properties>
			</Item>
